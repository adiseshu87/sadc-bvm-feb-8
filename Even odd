#include <stdio.h>

int main()
{
    int a[10],i,n;
    
    printf("enter arry size");
    scanf("%d",&n);
    printf("enter arry elements ");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("\n ");
    for(i=0;i<n;i++)
{
    if(a[i]%2==1)
    printf("%d ",a[i]);
}
printf("\t ");
for(i=0;i<n;i++)
{
    if(a[i]%2==0)
    printf("%d ",a[i]);
}
    return 0;
}
